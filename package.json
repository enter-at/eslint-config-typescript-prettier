{
    "name": "@enter-at/eslint-config-typescript-prettier",
    "version": "1.9.35",
    "description": "Enterat's ESLint config with TypeScript and Prettier support.",
    "main": "index.js",
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=16"
    },
    "files": [
